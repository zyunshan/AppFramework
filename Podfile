# Uncomment this line to define a global platform for your project
platform :ios, '8.0'

target ‘AppFramework’ do

pod 'Masonry’,'~>1.0.2’ #约束布局
pod 'AFNetworking', '~>3.1.0'   #网络请求
pod 'SDWebImage', '~>3.8.2' #图片处理
pod 'MJExtension', '~> 3.0.13'  #json model 转换
pod 'SVProgressHUD' #网络加载进度指示
pod 'IQKeyboardManager' #键盘弹出处理
pod 'MJRefresh' #上拉加载 下拉刷新
pod 'SDCycleScrollView' #循环视图
pod 'FSMediaPicker'
end

